{
    "formatters": {
        "simple": {
            "format": "[%(levelname)s][%(name)s] %(message)s",
            "colorize": false
        },
        "details": {
            "format": "[%(date)s] %(name)s.%(levelname)s: %(message)s",
            "colorize": true
        },
        "express": {
            "format": "[%(date)s]: %(message)s",
            "colorize": true
        },
        "scripts": {
            "format": "[%(date)s]: %(message)s",
            "colorize": true
        }
    },
    "handlers": {
        "terminal": {
            "class": "intel/handlers/console",
            "formatter": "details",
            "level": "DEBUG"
        },
        "express": {
            "class": "intel/handlers/console",
            "formatter": "express",
            "level": "INFO"
        }
    },
    "loggers": {
        "main": {
            "handlers": ["terminal"],
            "level": "DEBUG"
        },
        "express": {
            "handlers": ["express"],
            "level": "INFO"
        }
    }
}
